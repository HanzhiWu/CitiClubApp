<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.BusinessHallPage.EnterpriceInfo.EnterpriceInfoActivity">

    <com.example.citiclubapp.widgetLayout.InsideTitle
        android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:showDividers="end"
        android:orientation="vertical"
        android:divider="@drawable/u626">

        <com.example.citiclubapp.widgetLayout.ItemInfo
            android:id="@+id/credit_code"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="visible" />

        <com.example.citiclubapp.widgetLayout.ItemInfo
            android:id="@+id/company_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <com.example.citiclubapp.widgetLayout.ItemInfo
            android:id="@+id/register_code"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <com.example.citiclubapp.widgetLayout.ItemInfo
            android:id="@+id/company_address"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <com.example.citiclubapp.widgetLayout.ItemInfo
            android:id="@+id/company_tel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>


    </LinearLayout>


    <com.example.citiclubapp.widgetLayout.ItemInfo
        android:id="@+id/legalPerson_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <com.example.citiclubapp.widgetLayout.ItemInfo
        android:id="@+id/legalPerson_tel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


    <com.example.citiclubapp.widgetLayout.ItemInfo
        android:id="@+id/legalPerson_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

</LinearLayout>